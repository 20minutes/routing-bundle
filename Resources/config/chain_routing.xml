    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="router.chain.class">Symfony\Bundle\ChainRoutingBundle\Routing\ChainRouter</parameter>
    </parameters>
    <services>
        <service id="router" alias="router.chained" />
    </services>
</container>
